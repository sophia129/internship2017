<OptionsButton id="CorpButton" name="CorpDialog" />
	<REPORT>
		<SECTION>
			<HEADER>
				<LAYER Class="HD1">
					<ROWS>
						<COLUMN Val="[OptionsButton:CorpButton] [Lock] [DEF:ReportName]" Fmt="@" Colspan="4"/>
					</ROWS>
					<ROWS>
						<COLUMN Val="[LABEL:M1] vs. [LABEL:C1]" Colspan="4" Align="Center"/>
					</ROWS>
					<ROWS Filter="Dim:SIL">
						<COLUMN Val="[LABEL:S]" Colspan="4" Align="Center"/>
					</ROWS>
					<ROWS Filter="Dim:Sm">
